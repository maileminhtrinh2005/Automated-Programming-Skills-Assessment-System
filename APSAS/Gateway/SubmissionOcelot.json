{
  "Routes": [

    // doan nay la dung de nop bai + chay code
    {
      "DownstreamPathTemplate": "/api/Submission/Submit",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5090
        }
      ],
      "UpstreamPathTemplate": "/Submit",
      "UpstreamHttpMethod": [ "POST" ]
    },

    {
      "DownstreamPathTemplate": "/api/Submission/RunCode",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5090
        }
      ],
      "UpstreamPathTemplate": "/RunCode",
      "UpstreamHttpMethod": [ "POST" ]
    },

    // ddoan nay lay danh sach bai lam,  duoi la danh sach ket qua cho tung bai

    {
      "DownstreamPathTemplate": "/api/Result/GetSubmissions/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5090
        }
      ],
      "UpstreamPathTemplate": "/GetSubmissions/{id}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/Result/GetYourResult/{id}",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5090
        }
      ],
      "UpstreamPathTemplate": "/GetYourResult/{id}",
      "UpstreamHttpMethod": [ "GET" ]
    },
    {
      "DownstreamPathTemplate": "/api/Result/GetMySubmission",
      "DownstreamScheme": "http",
      "DownstreamHostAndPorts": [
        {
          "Host": "localhost",
          "Port": 5090
        }
      ],
      "UpstreamPathTemplate": "/GetMySubmission",
      "UpstreamHttpMethod": [ "GET" ]
    }
